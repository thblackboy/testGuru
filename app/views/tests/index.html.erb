<h1> Test List</h1>
<p>
  <%= link_to "New test", new_test_path %>
</p>
<%if @tests.present? %>
  <table>
    <tr>
      <td>Title</td>
      <td>Level</td>
      <td>Questions</td>
      <td colspan="3">Action</td>
    </tr>
    <%= render @tests %>
  </table>
  <% else %>
    <p>No tests</p>
<% end %>