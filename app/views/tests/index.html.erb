<h1> Test List</h1>
<%if @tests.present? %>
  <table class="table">
    <thead>
      <td class="sort-by-title pointer">
      Title
      <%= octicon 'arrow-up', class: 'text-success hide'%>
      <%= octicon 'arrow-down', class: 'text-success hide'%>
      </td>
      <td>Level</td>
      <td>Questions</td>
      <td>Actions</td>
    </thead>
    <tbody>
    <%= render @tests %>
    </tbody>
  </table>
  <% else %>
    <p>No tests</p>
<% end %>
